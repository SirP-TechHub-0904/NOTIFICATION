    <style>
        .card-title {
            float: left;
            font-size: 2rem;
            font-weight: 900;
            margin: 0;
        }

        .btn-group-sm > .btn, .btn-sm {
            padding: .25rem .5rem;
            font-size: 1.275rem;
            font-weight: bolder;
            line-height: 1.5;
            border-radius: .2rem;
        }

        .card-title {
            text-transform: uppercase !important;
        }
    </style>
